<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cesarynga.threading">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AsyncTaskActivity"
            android:label="@string/title_async_task" />
        <activity
            android:name=".HandlerThreadActivity"
            android:label="@string/title_handler_thread" />
        <activity
            android:name=".IntentServiceActivity"
            android:label="@string/title_intent_service" />

        <service
            android:name=".MyIntentService"
            android:exported="false" />
    </application>

</manifest>